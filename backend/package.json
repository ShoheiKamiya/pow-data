{
  "name": "docker_web_app",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "main": "server.js",
  "scripts": {
    "start": "yarn run build:live",
    "build:live": "nodemon --watch '*.ts' --exec 'ts-node' server.ts"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.16.1",
    "nodemon": "2.0.6",
    "pg": "^8.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.7",
    "@types/pg": "^7.14.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
